#
#
# Custom Mining in Volcano
#
#
on break:
	if event-block is in the region "ingor_volcano":
		if player's gamemode is survival:
			if event-block is emerald ore:
				cancel event
				set event-block to air
				drop 1 emerald with name "Emeraude Volcanique" with lore "Cet Emeraude provient||d'un volcan actif."
			else if event-block is iron ore:
				cancel event
				set event-block to air
				drop 1 iron ingot with name "Fer Volcanique" with lore "Ce Fer provient||d'un volcan actif."
			else if event-block is gold ore:
				cancel event
				set event-block to air
				drop 1 gold ingot with name "Or Volcanique" with lore "Cet Or provient||d'un volcan actif."
			else if event-block is diamond ore:
				cancel event
				set event-block to air
				drop 1 diamond with name "Diamant Volcanique" with lore "Ce Diamant provient||d'un volcan actif."
			else if event-block is lapis ore:
				cancel event
				set event-block to air
				drop 1 lapis with name "Lapis Volcanique" with lore "Ce Lapis provient||d'un volcan actif."
			else if event-block is redstone ore:
				cancel event
				set event-block to air
				drop 1 redstone with name "Redstone Volcanique" with lore "Cette redstone provient||d'un volcan actif."
			else if event-block is obsidian:
				cancel event
				message "Cette obsidienne est trop dure pour être cassée..."